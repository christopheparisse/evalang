                <annotationBlock who="ADU" xml:id="au0">
                    <u>
                        <seg>bah oui ça marche pas si c'est un un monsieur on dit que c'est une maman . </seg>
                    </u>
                </annotationBlock>
                <annotationBlock who="ADU" xml:id="au1">
                    <u>
                        <seg>ah oui mais faut la mettre dans le sens là sinon sinon j' ai pas reconnu . </seg>
                    </u>
                </annotationBlock>
                <annotationBlock who="ADU" xml:id="au2">
                    <u>
                        <seg>et+puis j' en ai j' en ai une autre qui s' appelle Stéphanie . </seg>
                    </u>
                </annotationBlock>
                <annotationBlock who="ADU" xml:id="au3">
                    <u>
                        <seg>ouais mais pour être être jumeaux il faut être frère et soeur . </seg>
                    </u>
                </annotationBlock>
                <annotationBlock who="ADU" xml:id="au4">
                    <u>
                        <seg>donc si tu es gentille tu auras des cadeaux ça veut dire . </seg>
                    </u>
                </annotationBlock>
                <annotationBlock who="ADU" xml:id="au5">
                    <u>
                        <seg>ah maman elle aime pas c'est normal hein c'est pour les petits . </seg>
                    </u>
                </annotationBlock>
                <annotationBlock who="ADU" xml:id="au6">
                    <u>
                        <seg>bah si elle est née avant moi elle est plus grande . </seg>
                    </u>
                </annotationBlock>
                <annotationBlock who="ADU" xml:id="au7">
                    <u>
                        <seg>mais tu voulais faire quoi avec toutes les cordes là ? </seg>
                    </u>
                </annotationBlock>
                <annotationBlock who="ADU" xml:id="au8">
                    <u>
                        <seg>et on dit que c'est des jumelles ou des jumeaux ? </seg>
                    </u>
                </annotationBlock>
                <annotationBlock who="ADU" xml:id="au9">
                    <u>
                        <seg>elle aura deux ans et toi tu auras quel âge ? </seg>
                    </u>
                </annotationBlock>
                <annotationBlock who="ADU" xml:id="au10">
                    <u>
                        <seg>c' est+ce+que tu joues à des fois à l' école . </seg>
                    </u>
                </annotationBlock>
                <annotationBlock who="ADU" xml:id="au11">
                    <u>
                        <seg>bah ils veulent jouer à quoi à l' école alors ? </seg>
                    </u>
                </annotationBlock>
                <annotationBlock who="ADU" xml:id="au12">
                    <u>
                        <seg>attends Esther c'est la soeur ou la cousine d' Albane ? </seg>
                    </u>
                </annotationBlock>
                <annotationBlock who="ADU" xml:id="au13">
                    <u>
                        <seg>O . K . tu veux qu' on s' écoute ? </seg>
                    </u>
                </annotationBlock>
                <annotationBlock who="ADU" xml:id="au14">
                    <u>
                        <seg>et+puis tu as dit quand tu seras grande après . </seg>
                    </u>
                </annotationBlock>
                <annotationBlock who="ADU" xml:id="au15">
                    <u>
                        <seg>et tu as eu quoi d' autres comme cadeaux ? </seg>
                    </u>
                </annotationBlock>
                <annotationBlock who="ADU" xml:id="au16">
                    <u>
                        <seg>elle a les mêmes que toi Mathilde de cousines . </seg>
                    </u>
                </annotationBlock>
                <annotationBlock who="ADU" xml:id="au17">
                    <u>
                        <seg>c'est la maison que tu as commandée pour Noël ? </seg>
                    </u>
                </annotationBlock>
                <annotationBlock who="ADU" xml:id="au18">
                    <u>
                        <seg>ah bah c'est comme quand tu fais la vaisselle . </seg>
                    </u>
                </annotationBlock>
                <annotationBlock who="ADU" xml:id="au19">
                    <u>
                        <seg>presque des jumelles ça veut dire quoi ça ? </seg>
                    </u>
                </annotationBlock>
